<h1>Homepage</h1>

{{#each postSer as |posts| }}
<div class="row mb-4 post project">
  <div class="col-md-5">
    <h2>
      <a data-userid="{{posts.id}}">{{posts.title}}</a>
    </h2>
    <p>Posted by {{posts.user_id}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{posts.body}}
    </p>
    <div>
        <button data-id="{{posts.id}}">Delete Post</button>
     </div>   
  </div>
</div>
{{/each}}

<script src="./js/modifyBlogPost.js"></script>